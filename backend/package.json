{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "preinstall": "npm i -g pm2 && pm2 install pm2-logrotate",
    "start": "pm2-runtime start ecosystem.config.js --env production && pm2 logs all",
    "lint": "eslint --fix .",
    "migrate": "npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "db:drop": "npx sequelize db:drop",
    "db:create": "npx sequelize db:create",
    "test": "jest --forceExit --runInBand"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "aws-sdk": "^2.441.0",
    "bcrypt": "^3.0.6",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "~4.13.1",
    "express-promise-router": "^3.0.3",
    "http-errors": "^1.7.2",
    "ioredis": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "minio": "^7.0.8",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "mysql2": "^1.6.5",
    "nodemon": "^1.18.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^3.5.1",
    "sendgrid": "^5.2.3",
    "sequelize": "^5.7.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "sequelize-cli": "^5.4.0",
    "supertest": "^4.0.2"
  }
}
